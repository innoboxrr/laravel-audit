{
    "name": "innoboxrr/laravel-audit",
    "description": "Módulo de auditoría",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Innoboxrr\\LaravelAudit\\": "src/",
            "Innoboxrr\\LaravelAudit\\Database\\Factories\\": "database/factories/",
            "Innoboxrr\\LaravelAudit\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "Homero Raul",
            "email": "h.raulvc@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "innoboxrr/larapack-generator": "^2.0 || ^3.0 || ^4.0 || ^5.0",
        "orchestra/testbench": "^8.0 || ^9.0"
    },
    "require": {
        "doctrine/dbal": "^3.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Innoboxrr\\LaravelAudit\\Providers\\AppServiceProvider",
                "Innoboxrr\\LaravelAudit\\Providers\\AuthServiceProvider",
                "Innoboxrr\\LaravelAudit\\Providers\\EventServiceProvider",
                "Innoboxrr\\LaravelAudit\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Innoboxrr\\LaravelAudit\\Tests\\": "tests/"
        }
    }
}
